{
  "name": "marcelofabianov/php-clean-architecture",
  "type": "project",
  "license": "MIT",
  "require": {
    "php": "8.1.*",
    "ext-json": "*",
    "ext-pdo": "*",
    "nesbot/carbon": "^2.62",
    "ramsey/uuid": "^4.5",
    "slim/psr7": "^1.5",
    "slim/slim": "^4.10",
    "vlucas/phpdotenv": "^5.4"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5"
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Test\\": "test/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  }
}
